{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "settings": {
    "tableName": "code4-dev",
    "bucketName": "code4-dev",
    "intervalInSeconds": 30,
    "resultsCacheIntervalInSeconds": 60,
    "turnoutCacheIntervalInSeconds": 15
  },
  "DynamoDb": {
    "LocalMode": true,
    "LocalServiceUrl": "http://localhost:8000"
  },
  "AWS.Logging": {
    "Region": "eu-central-1",
    "LogGroup": "HandsOnLogGroup",
    "IncludeLogLevel": true,
    "IncludeCategory": true,
    "IncludeNewline": true,
    "IncludeException": true,
    "IncludeEventId": false,
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "None"
    },
    "Console": {
      "LogLevel": {
        "Default": "Error",
        "System": "Information",
        "Microsoft": "None"
      }
    },
    "Debug": {
      "LogLevel": {
        "Default": "Trace",
        "System": "Information",
        "Microsoft": "None"
      }
    }
  }
}